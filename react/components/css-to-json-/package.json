{
  "name": "@thinkeloquent/css-to-json",
  "version": "0.1.0",
  "main": "./index.js",
  "scripts": {
    "pretest": "npm run lint && npm run webpack && npm run build",
    "test": "npm run jest",
    "lint": "eslint --ignore-path .eslintignore src",
    "jest": "jest --coverage",
    "jest:unit": "npm run jest --- -t '#unit'",
    "jest:watch": "npm run jest:unit -- --watch",
    "clean:dist": "rm -rf ./dist && mkdir -p ./dist",
    "build": "npm run clean:dist && npm run es5",
    "uat": "npm run webpack:dev",
    "webpack": "webpack --progress --colors --config ./webpack.devserver.config.js",
    "webpack:dev": "webpack-dev-server --progress --colors --config ./webpack.devserver.config.js",
    "docs": "TBD",
    "es5": "node ./node_modules/babel-cli/bin/babel ./src --out-dir ./dist --copy-files --ignore .spec"
  },
  "license": "ISC",
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.0",
    "chalk": "^2.1.0",
    "documentation": "^5.3.1",
    "eslint": "^4.2.0",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^21.0.2",
    "jest-junit": "^3.0.0",
    "mocha": "^3.5.2",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.6",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "dependencies": {
    "camelcase": "^2.1.1",
    "css": "^2.2.1"
  },
  "jest": {
    "verbose": true,
    "testRegex": ".*/__tests__/.*\\.(spec\\.js)$",
    "transformIgnorePatterns": [
      "/node_modules/",
      "webpack.config.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "webpack.config.js",
      ".spec.js"
    ],
    "testResultsProcessor": "jest-junit",
    "collectCoverageFrom": [
      "./src/*.js",
      "./src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "./testresults/test-results.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  }
}
